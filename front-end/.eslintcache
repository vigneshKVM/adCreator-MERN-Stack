\"\r\n                                            suppressContentEditableWarning={true}\r\n                                            fontColor={fontColor}\r\n                                            fontSize={fontSize}\r\n                                            onInput={(e) => {\r\n                                                setDefaultText(e.target.textContent)\r\n                                                console.log(e.target.textContent)\r\n                                            }}\r\n                                        >\r\n                                            {defaultText}\r\n                                        </WorkSpaceTextBox>\r\n                                    </Underlined>\r\n                                </Italic>\r\n                            </Bold>\r\n                        </WorkSpaceElementBorder>\r\n                        <EditableElements isSelected={selected}>\r\n                            <BoldIcon func={setBold} state={bold}/>\r\n                            <ItalicIcon func={setItalic} state={italic}/>\r\n                            <UnderlinedIcon func={setUnderlined} state={underlined}/>\r\n                            <FontSize value={fontSize} func={setFontSize}/>\r\n                            {/*<FontColorIcon onClick={() => setShowColorPalate(!showColorPalate)} showColorPalate={showColorPalate}/>*/}\r\n                        </EditableElements>\r\n                        <RotateElementIcon isSelected={selected}/>\r\n                    </>\r\n\r\n                </OutsideClickAlerter>\r\n            </>\r\n        </Dragable>\r\n    )\r\n}\r\n\r\nexport default WorkSpaceElementText;\r\n","D:\\Vignesh Files\\Website Development\\Freelance projects\\frontend-post-creator\\src\\shared\\components\\textComponents\\BoldIcon.js",[],"D:\\Vignesh Files\\Website Development\\Freelance projects\\frontend-post-creator\\src\\shared\\components\\includes\\OutsideClickAlerter.js",[],"D:\\Vignesh Files\\Website Development\\Freelance projects\\frontend-post-creator\\src\\shared\\components\\textComponents\\ItalicIcon.js",[],"D:\\Vignesh Files\\Website Development\\Freelance projects\\frontend-post-creator\\src\\shared\\components\\textComponents\\fontSize.js",[],"D:\\Vignesh Files\\Website Development\\Freelance projects\\frontend-post-creator\\src\\shared\\components\\includes\\dragable.js",["146","147","148","149","150"],"import React, {useState, useEffect} from 'react';\r\nimport {DragableElementContainer} from \"../../global.style\";\r\n\r\nconst Dragable = (props) => {\r\n    const [diffX, setDiffX] = useState(0);\r\n    const [diffY, setDiffY] = useState(0);\r\n    const [drag, setDrag] = useState(false);\r\n    const [style, setStyle] = useState({});\r\n\r\n    let left = 0; let top = 0;\r\n    const dragStart = (e) => {\r\n        setDiffX(e.screenX - e.currentTarget.getBoundingClientRect().left);\r\n        setDiffY(e.screenY - e.currentTarget.getBoundingClientRect().top);\r\n        let topLeft = document.getElementById('topLeft1');\r\n        let topRight = document.getElementById('topRight1');\r\n        let bottomLeft = document.getElementById('bottomLeft1');\r\n        let bottomRight = document.getElementById('bottomRight1');\r\n        setDrag(true);\r\n        console.log('Dragable: Drag Start')\r\n    }\r\n\r\n    const dragging = (e) => {\r\n        if (drag) {\r\n            left = e.screenX - diffX;\r\n            top = e.screenY - diffY;\r\n            setStyle((state) => ({left, top}));\r\n            console.log('Dragable: dragging', style);\r\n        }\r\n    }\r\n\r\n    const dragEnd = (e) => {\r\n        setDrag(false);\r\n        console.log('Dragable: Drag End')\r\n    }\r\n\r\n    return (\r\n        <DragableElementContainer style={style} onMouseDown={dragStart} onMouseMove={dragging} onMouseUp={dragEnd}>\r\n            {props.children}\r\n        </DragableElementContainer>\r\n    )\r\n}\r\n\r\nexport default Dragable;\r\n","D:\\Vign